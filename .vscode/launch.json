    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: ann-benchmarks-aerospike",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "cwd": "${workspaceFolder}",
            "args": ["--algorithm", "aerospike",
                     "--max-n-algorithms", "4",
                     "--runs", "5",
                     "--dataset", "random-xs-20-angular",
                     "--local",
                     "--force",
                     "--timeout", "300"],
            "console": "integratedTerminal"
        },
